import java.util.Scanner;

public class LabelTanggal {
    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);
        String[] bulan = {
                "Januari",
                "Februari",
                "Maret",
                "April",
                "Mei",
                "Juni",
                "Juli",
                "Agustus",
                "September",
                "Oktober",
                "November",
                "Desember",
        };

        int tanggalnya = input.nextInt();
        int bulannya = input.nextInt();
        int tahunnya = input.nextInt();
        System.out.print(tanggalnya + bulan[bulannya-1] + tahunnya);

    }
}
