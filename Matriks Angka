import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
      Scanner Input = new Scanner(System.in);
      int sebanyak = Input.nextInt(); 

        for (int i = 0; i < sebanyak ; i++) {
            for (int j = 0; j < sebanyak ; j++) {
                System.out.print(j + " ");
            }
            System.out.println();
        }

    }
}
