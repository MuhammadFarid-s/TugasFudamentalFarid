import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);
        String[] bulan = {
                "Januari",
                "Februari",
                "Maret",
                "April",
                "Mei",
                "Juni",
                "Juli",
                "Agustus",
                "September",
                "Oktober",
                "November",
                "Desember",
        };
        Scanner bulannya = new Scanner(System.in);
        int sibulan = bulannya.nextInt();
        int bukan = 0;
        if (sibulan > 0 && sibulan <=12){
          System.out.println(bulan[sibulan-1]);
        } else {
        System.out.println( "Nomor tidak valid" );
}
    }
}
